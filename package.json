{
  "name": "@swimm/ui",
  "version": "2.18.3",
  "description": "Swimm's Design System and Component Library",
  "license": "MIT",
  "scripts": {
    "dev": "vuepress dev docs",
    "build": "vite build",
    "watch": "pnpm build --watch",
    "lint": "eslint . --ext .vue,.js --fix --ignore-path .gitignore",
    "test": "vitest watch",
    "test:ci": "vitest run",
    "docs:build": "vuepress build docs",
    "prepare": "husky install",
    "commit": "cz"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/swimm-ui.d.ts",
  "main": "./dist/swimm-ui.umd.cjs",
  "module": "./dist/swimm-ui.js",
  "exports": {
    ".": {
      "types": "./dist/swimm-ui.d.ts",
      "import": "./dist/swimm-ui.js",
      "require": "./dist/swimm-ui.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/styles/index.css": "./dist/styles/index.css"
  },
  "dependencies": {
    "floating-vue": "^2.0.0-beta.20",
    "fontaweswimm": "github:swimmio/fontaweswimm#2.0.6"
  },
  "packageManager": "pnpm@7.14.2",
  "engines": {
    "node": "^16",
    "pnpm": ">=7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/glob": "^8.0.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^16.18.11",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "@vuepress/client": "2.0.0-beta.60",
    "@vuepress/plugin-register-components": "2.0.0-beta.60",
    "@vuepress/theme-default": "2.0.0-beta.60",
    "@vuepress/utils": "2.0.0-alpha.1",
    "@vueuse/core": "^9.9.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "glob": "^8.0.3",
    "glob-promise": "^5.0.1",
    "husky": "^8.0.3",
    "jsdom": "^20.0.3",
    "markdown-it-container": "^3.0.0",
    "prettier": "^2.8.1",
    "semantic-release": "^19.0.5",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.23.4",
    "vue": "^3.2.45",
    "vue-docgen-api": "^4.56.2",
    "vue-tsc": "^1.0.19",
    "vuepress": "2.0.0-beta.60"
  },
  "peerDependencies": {
    "vue": "^3.2.40"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "resolutions": {
    "minimist": "1.2.6"
  }
}
